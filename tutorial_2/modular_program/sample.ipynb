   "source": [
    "def fact(num):\n",
    "\n",
    "    if num == 1:\n",
    "        return num\n",
    "    else:\n",
    "        return num * fact(num - 1)\n",
    "\n",
    "def pal(x):\n",
    "    x1 = x[::-1]\n",
    "    if x1 == x:\n",
    "        return 'palindrome'\n",
    "    else:\n",
    "        return 'not palindrome'"
   ]
